@model IEnumerable<Owner>
@{
	ViewData["Title"] = "Index";
}
<a asp-controller="Owner" asp-action="Create"> aaaa</a>

<div class="container p-3">
	<div class="row pt-4">
		<div class="col-6" ">
			<h2 class="text-primary">Owner List</h2>
		</div>
		
	</div>
</div>
<table class="table table-bordered table-striped" style="width:100%">
	<thead>
		<tr>
			<th>
				Owner Name
			</th>
			<th>
				Driver License
			</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var obj in Model){
		<tr>
			<th width="50%">
				@obj.FirstName
				@obj.LastName
			</th>
			<th width="30%">
				@obj.DriverLicense
			</th>
		</tr>
		}
	</tbody>
</table>
